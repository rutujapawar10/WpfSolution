<Application x:Class="WpfDemoApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WpfDemoApp"
             StartupUri="Bindings/EligibilityPersonWindow.xaml">
    <Application.Resources>
       
        <ResourceDictionary>
            <LinearGradientBrush x:Key="B1" StartPoint=".5,0" EndPoint=".5,1">
                <GradientStop Color="Aquamarine" Offset="0"></GradientStop>
                <GradientStop Color="Blue" Offset="1.5"></GradientStop>
            </LinearGradientBrush>
            <LinearGradientBrush x:Key="B2" StartPoint=".5,0" EndPoint=".5,1">
                <GradientStop Color="Red" Offset="0"></GradientStop>
                <GradientStop Color="Orange" Offset="0.5"></GradientStop>
                <GradientStop Color="Violet" Offset="1.5"></GradientStop>
            </LinearGradientBrush> 
              <ResourceDictionary.MergedDictionaries>
            <ResourceDictionary Source="DemoDictionary.xaml"></ResourceDictionary>
        </ResourceDictionary.MergedDictionaries>
   
        </ResourceDictionary>
      </Application.Resources> 
</Application>
